<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.zucc.pump.mapper.DeviceTypeMapper">
    <select id="getAllList" resultType="DeviceType">
        SELECT * FROM device_type
    </select>
    <select id="queryById" resultType="DeviceType" parameterType="Integer">
        SELECT * FROM device_type WHERE id=#{id}
    </select>
</mapper>