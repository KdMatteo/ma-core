<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.zucc.pump.mapper.DeviceAttrTypeMapper">
    <select id="getAllList" resultType="DeviceAttrType">
        SELECT * FROM device_attr_type
    </select>
    <select id="queryById" parameterType="Integer" resultType="DeviceAttrType">
        SELECT * FROM device_attr_type WHERE id =#{id}
    </select>

</mapper>